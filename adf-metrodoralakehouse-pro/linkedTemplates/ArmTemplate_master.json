			"type": "string",
			"defaultValue": "https://api.aircall.io/"
		},
		"ls_Aircall_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "c668cd9a3a86775ddb72333278d0b7bb"
		},
		"ls_AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
